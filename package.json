{
  "name": "enlite_proto",
  "version": "1.0.0",
  "description": "",
  "author": "prisme",
  "license": "ISC",
  "main": "index.js",
  "scripts": {
    "start": "budo index.js:bundle.js --live",
    "build": "browserify index.js | uglifyjs -m -c warnings=false > bundle.js"
  },
  "browserify": {
    "transform": [
      "aliasify",
      [
        "installify",
        {
          "save": true
        }
      ],
      [
        "jadeify",
        {
          "compileDebug": true,
          "pretty": true
        }
      ]
    ]
  },
  "aliasify": {
    "aliases": {
      "lib": "./lib",
      "npm": "./node_modules/"
    },
    "replacements": {
      "components/(\\w+)": "./components/$1/index.js"
    }
  },
  "dependencies": {
    "budo": "^8.3.0",
    "components": "^0.1.0",
    "gsap": "^1.19.0",
    "installify": "^1.0.3",
    "jade": "^1.11.0",
    "lib": "^0.1.1",
    "node-static": "^0.7.8",
    "prismic-toolbar": "prismicio/prismic-toolbar#274d154",
    "prismic.io": "^3.1.3",
    "pubsub": "^3.2.1",
    "swiper": "^3.3.1"
  },
  "devDependencies": {
    "aliasify": "^2.0.0",
    "browserify": "^13.1.0",
    "budo": "^8.3.0",
    "installify": "^1.0.3",
    "jadeify": "^4.6.0",
    "uglify-js": "^2.7.0"
  }
}
